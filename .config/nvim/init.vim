set nu
set expandtab
set mouse=a
set tabstop=4
set softtabstop=4
set shiftwidth=4
colorscheme vim
"call plug#begin("~/.config/nvim/autoload/plugged")
"Plug 'nvim-treesitter/nvim-treesitter', {'do': ':TSUpdate'}
"Plug 'LunarWatcher/auto-pairs'
"call plug#end()
"lua require'nvim-treesitter.configs'.setup { ensure_installed = { "c", "cpp" }, highlight = { enable = true, }, indent = { enable = true } }
